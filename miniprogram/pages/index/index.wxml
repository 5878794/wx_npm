


<view style="padding:0 20rpx;">
    <b-input class="b_input" icon="../../images/5@1x.png" type="text" placeholder="电话号码" data-id="222" name="电话" id="name" value="{{input.name}}" rule="must,min:1,max:20,str"  err="请输入用户名" bindmychange="aaa"></b-input>
    <b-input class="b_input" icon="../../images/5@1x.png" type="password" placeholder="电话号码" name="密码" id="password" value="{{input.password}}" ></b-input>
    <b-input class="b_input" icon="../../images/5@1x.png" type="number" placeholder="电话号码" name="数字" id="number" value="{{input.number}}" ></b-input>
    <b-input class="b_input" icon="../../images/5@1x.png" type="idcard" placeholder="电话号码" name="身份证" id="idcard" value="{{input.idcard}}" ></b-input>
    <b-input class="b_input" icon="../../images/5@1x.png" type="digit" placeholder="电话号码" name="小数" id="digit" value="{{input.digit}}" ></b-input>
    <b-input class="b_input" icon="../../images/5@1x.png" type="textarea" placeholder="电话号码" name="文本框" id="textarea" value="{{input.textarea}}" ></b-input>

    <b-select class="b_input" icon="../../images/5@1x.png" type="select" placeholder="电话号码" selectData="{{selectData}}" name="单选" id="select" rule="must,min:3" value="{{input.select}}" ></b-select>
    <b-multi-select class="b_input" icon="../../images/5@1x.png" cascade="true" type="multiSelector" placeholder="电话号码" selectData="{{selectData3}}" name="单选" id="mSelect" rule="must" value="{{selectValue3}}"  ></b-multi-select>
    <b-multi-select class="b_input" icon="../../images/5@1x.png" cascade="false" type="multiSelector" placeholder="电话号码" selectData="{{selectData4}}" name="单选" id="m1Select" rule="must" value="{{selectValue4}}"  ></b-multi-select>
    <b-date class="b_input" icon="../../images/5@1x.png" type="time" start="08:00" end="21:00" placeholder="电话号码" name="密码" id="time" value="09:03" ></b-date>
    <b-date class="b_input" icon="../../images/5@1x.png" type="date" start="2020-11-11" end="2022-11-11" placeholder="电话号码" name="密码" id="date" value="2020-12-12" ></b-date>
    <b-area class="b_input" customItem="全部" icon="../../images/5@1x.png"  placeholder="电话号码" name="密码" id="area" value='四川省,成都市,金牛区' ></b-area>
</view>




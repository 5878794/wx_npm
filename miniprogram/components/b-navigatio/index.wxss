
@import "../../css/box.wxss";


.title{
    position: fixed; left:0; top:0; z-index: 10000;
    width: 100%;
    height: 200rpx;
    text-align: center;
    /*line-height: 120rpx;*/
    font-size: 40rpx;
    font-weight: bold;
    padding-top:40rpx;
    overflow: hidden;
}
.title_main{
    position: relative; width: 100%;
}
._title{
    width: 100%;
    height: 200rpx;
}
.image{
    display: block; width: 16rpx; height: 32rpx;
    padding-left: 38rpx;
}
.title_left,
.right{
    width: 60rpx;
    /*height: 120rpx;*/
}

.bg_img{
    position: absolute; left:0; top:0;
    width: 100%;
}

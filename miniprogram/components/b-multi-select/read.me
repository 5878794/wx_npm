
//可以用lib的jq库


//multi-select
//数据控制层级 2-4级   最好不要超过4级，显示不下
//带级联关系


<b-multi-select
    class="b_input"
    icon="../../images/5@1x.png"
    type="multiSelector"
    placeholder="电话号码"
    name="电话"
    id="name"
    selectData="{{selectData}}"         //[{key:11,value:'a',children:[...]},...]
    value="{{key}}"                     //key的值   str:"1,1,1"
    rule="must"
    err="请输入用户名"
    bindmychange='function'
></b-multi-select>

